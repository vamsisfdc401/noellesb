    <rules>
        <fullName>AD Board Pack Prep</fullName>
        <actions>
            <name>AD_Approval_Status_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Acquisition_Deal__c.Acquisition_Deal_Stage__c</field>
            <operation>equals</operation>
            <value>Board Pack Preparation</value>
        </criteriaItems>
        <criteriaItems>
            <field>Acquisition_Deal__c.Approval_Notes__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <description>Workflow rule update the Acquisition Deal Approval Status when the Acquisition deal Stage equals Board Pack preparation.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
