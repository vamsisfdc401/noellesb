<aura:component implements="force:appHostable,force:hasRecordId,flexipage:availableForAllPageTypes" access="global" controller="DGF_TPRPricingController">
    
    <ltng:require scripts="{!join(',',$Resource.jQuery_Plugin,$Resource.jQuery_StickyHeader)}" afterScriptsLoaded="{!c.scriptLoaded}"/>
    
    <aura:attribute name="recordId" type="Id"/> 
    <aura:attribute name="numberOfRecords" type="integer" description="to count total number of records"/> 
    <aura:attribute name="showSpinner" type="boolean" default="false" description="use for displaying or hiding spinner"/>   
    <aura:attribute name="showViewAll" type="boolean" default="true" description="show view all option"/>
    <aura:attribute name="territoryFilter" type="String" description="used to store the value selected in Territory filter"/>
    <aura:attribute name="territoryList" type="String[]" description="used to store the TPR Prices set."/>
    <aura:attribute name="resFilter" type="String" description="used to store the value selected in Resolution filter"/>
    <aura:attribute name="platformFilter" type="String" description="used to store the value selected in Platform filter"/>
    <aura:attribute name="WSPorSRPFilter" type="String" description="used to store the value selected in WSP vs SRP filter"/>
    
    <aura:attribute name="StandardorTPRFilter" type="String" description="used to store the value selected in Standard vs TPR filter"/>
    
    <aura:attribute name="accountFilter" type="String" description="used to store the value selected in Account filter"/>
    <aura:attribute name="us" type="Boolean" default="true" description="used for displaying prices where Territory is US"/>
    <aura:attribute name="ca" type="Boolean" default="true" description="used for displaying prices where Territory is CA"/>
    <aura:attribute name="est" type="Boolean" default="true" description="used for displaying prices where Platform is EST"/>
    <aura:attribute name="vod" type="Boolean" default="false" description="used for displaying prices where Platform is VOD"/>
    <aura:attribute name="sd" type="Boolean" default="true" description="used for displaying prices where Resolution is SD"/>
    <aura:attribute name="hd" type="Boolean" default="true" description="used for displaying prices where Resolution is HD"/>
    <aura:attribute name="uhd" type="Boolean" default="true" description="used for displaying prices where Resolution is UHD"/>
    <aura:attribute name="xd" type="Boolean" default="false" description="used for displaying prices where Resolution is 3D"/>
    <aura:attribute name="wsp" type="Boolean" default="true" description="used for displaying WSPprices "/>
    <aura:attribute name="srp" type="Boolean" default="true" description="used for displaying SRP prices "/>
    
    <aura:attribute name="Standard" type="Boolean" default="true" description="used for displaying Standard prices "/>
    <aura:attribute name="TPR" type="Boolean" default="true" description="used for displaying TPR prices "/>
    
    
    <aura:attribute name="national" type="Boolean" default="true" description="used for displaying National prices "/>
    <aura:attribute name="iTunes" type="Boolean" default="false" description="used for displaying iTunes prices "/>
    <aura:attribute name="changedValue" type="String"/>
    <aura:attribute name="fieldName" type="String"/>
    <aura:attribute name="CodeError" type="string"  description="use for display no record found message"/>
    <aura:attribute name="isCodeError" type="boolean" default="false" description="use for display no record found message"/>
    <aura:attribute name="successMessage" type="string"  description="use for display success message"/>
    <aura:attribute name="successful" type="boolean" default="false" description="use for display success message"/>
    <aura:attribute name="recType" type="String" description="holds the value of the promotion recordType"/>
    <aura:attribute name="recordsToDisplay" type="List" description="use for store and display list return from server"/> 
    <aura:attribute name="allRecords" type="List" description="stores all the promotion products"/> 
    <aura:attribute name="scrollCheckIntervalId" type="String" />
    <aura:attribute name="prodCount" type="Integer"/>
    <aura:handler event="aura:waiting" action="{!c.showSpinner}" />
    <aura:handler event="aura:doneWaiting" action="{!c.hideSpinner}" />  
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:attribute name="isjQueryLoaded" type="Boolean" default="false"/>
    <aura:attribute name="restrictedProfile" type="Boolean" default="false"/>
    <aura:attribute name="tprGreater" type="Boolean"  default="False"/>
    <aura:handler event="aura:locationChange" action="{!c.update}"/>
    
    <aura:attribute name="selectedRow" type="String" default="none"/>
    <aura:attribute name="assending" type="Boolean" default="false"/>
    <!--DFOR-1445-->
    <aura:attribute name="accountValue" type="String"/>
    <aura:attribute name="actName" type="String"/>
    <aura:attribute name="accountValues" type="Promotion_Account__c[]"/>
    <!-- DFOR-1445 -->
    
    <aura:attribute name="searchResultMaster" type="List" description="use for store and display list return from server"/>    
    <aura:attribute name="pageSize" type="integer" default="25" description="default number of records to be displayed on a page"/>
    <aura:attribute name="totalNumberOfRecord" type="integer" default="0" description="use for display Number of records"/>

    <!--<aura:handler event="aura:locationChange" action="{!c.doInit}"/>-->
    <aura:attribute name="setIntervalId" type="String"/>
    <div class="slds-border_top slds-border_right slds-border_bottom slds-border_left" style="border-radius: 5px;">
        <article class="slds-card " >
            <aura:if isTrue="{!v.isCodeError}">
                <ui:message title="Error" severity="error" closable="false">        
                    {!v.CodeError}   
                </ui:message>
            </aura:if>
            <aura:if isTrue="{!v.successful}">
                <ui:message title="Info" severity="info" closable="false">        
                    {!v.successMessage}   
                </ui:message>
            </aura:if>
            <div class="slds-card__header slds-grid">    
                <header class="slds-media slds-media_center slds-has-flexi-truncate slds-m-top_xx-small slds-p-left_small">                      
                    <div class="{!v.showSpinner ? 'slds-media__figure slds-hide' : 'slds-media__figure slds-show'}">
                        <span class="slds-icon_container slds-icon-standard-contact" title="description of icon when needed">
                            <lightning:icon iconName="custom:custom62" size="small" alternativeText="TPR Products"/>
                        </span>
                    </div>
                    <div class="slds-media__body slds-m-top_xxx-small slds-grid slds-size--4-of-4">
                        <div>
                            <h2>                        
                                <a href="javascript:void(0);" class="slds-card__header-link slds-truncate" title="TPR Products">                            
                                    <span class="slds-text-heading_small">TPR Products ({!v.numberOfRecords}/{!v.prodCount})
                                        
                                    </span>
                                </a>
                            </h2> 
                        </div>
                        <aura:if isTrue="{!v.restrictedProfile==false}">
                            <div >
                                 <!--changed class slds-m-left_xx-large from to slds-m-left_none for issue #1 USST-3349  -->
                                <button aura:id="btnSearchId" type="button" class=" slds-m-left_none slds-button slds-button--neutral" onclick="{!c.addProds}" target="_blank">Add/Remove Products</button>
                            </div>
                            <div style="{!v.recType!='Free Episode' ? '' : 'display:none;'}">
                                <button aura:id="btnSearchId" type="button"  class="slds-button slds-m-left_medium slds-button--neutral" onclick="{!c.bulkTPR}">Bulk TPR Pricing</button>
                            </div>
                            <div style="margin-left:-2%;">
                                <!--changed class slds-m-left_xx-large from to slds-m-left_large for issue #1 USST-3349  -->
                                <button aura:id="btnSearchId" type="button"  class=" slds-m-left_large slds-button slds-button--brand" onclick="{!c.saveAll}" target="_blank">Save</button>
                            </div>
                        </aura:if>
                        
                        
                        <!-- DFOR -1445 -->
                        <div style="margin-left:2%;" >
                         <aura:if isTrue="{!v.recType=='Account Promotion'}">
                        <div class="slds-grid">
                            <aura:if isTrue="{!!(empty(v.actName))}">
                                <label class="slds-form-element__label" >Account</label>
                            <ui:inputText aura:id="actId1" value="{!v.actName}" disabled="true"/>
                                </aura:if>
                                <aura:if isTrue="{!empty(v.actName)}">
                          <label class="slds-form-element__label" >Account</label>
                        <ui:inputSelect aura:id="actId" change="{!c.onAccountChange}"> <!-- removed style attribute class="slds-size_1-of-2" for issue #1 USST-3349 -->
                            <ui:inputSelectOption text="--None--" label="--None--" />
                            <aura:iteration items="{!v.accountValues}" var="act">
                        	<ui:inputSelectOption text="{!act.Account__c}" label="{!act.Account__r.Name}"  />
                        </aura:iteration>
                    </ui:inputSelect>
                            </aura:if>
                        </div> 
                        </aura:if>
                        <!-- DFOR-1445 -->
                      </div>  
                        
                    </div>       
                    
                    <aura:if isTrue="{!v.showViewAll != true}">
                        <div style="margin-left : -18%;">
                            <button id="btnSearch" type="button"  onclick="{!c.cancel}" class="slds-button slds-button--brand">Back to Promotion</button>
                        </div>
                    </aura:if>
                    
                </header> 
            </div>
            
            <div class="slds-m-bottom--xxx-small slds-no-flex slds-grid slds-grid_pull-padded slds-m-left--xx-small slds-size--5-of-5 slds-clearfix" >
                <div class="slds-col--padded slds-size--1-of-6">
                    Territory:
                    <ui:inputSelect aura:id="filter" multiple="true" value="{!v.territoryFilter}" change="{!c.onFilterChange}" > 
                         <aura:iteration items="{!v.territoryList}" var="terr">
                        	<ui:inputSelectOption text="{!terr}" label="{!terr}" />
                        </aura:iteration>  
                    </ui:inputSelect>    
                </div>  
                <div class="slds-col--padded slds-size--1-of-6">
                    Resolution:
                    <ui:inputSelect aura:id="filter" multiple="true" value="{!v.resFilter}" change="{!c.onFilterChange}" >
                        
                        <ui:inputSelectOption text="SD" label="SD" value="true"/>
                        <ui:inputSelectOption text="HD" label="HD" value="true"/>   
                        <ui:inputSelectOption text="ultra" label="UHD" value="true"/>   
                        <ui:inputSelectOption text="3D" label="3D" value="true"/> 
                    </ui:inputSelect>    
                </div>
                <div class="slds-col--padded slds-size--1-of-6">
                    WSP vs SRP:
                    <ui:inputSelect aura:id="filter" multiple="true" value="{!v.WSPorSRPFilter}" change="{!c.onFilterChange}" >
                        
                        <ui:inputSelectOption text="WSP" label="WSP" value="true" />
                        <ui:inputSelectOption text="SRP" label="SRP" value="true"/>   
                    </ui:inputSelect>    
                </div>
                <div class="slds-col--padded slds-size--1-of-6">
                    Platform:
                    <ui:inputSelect aura:id="filter" multiple="true" value="{!v.platformFilter}" change="{!c.onFilterChange}"  >
                        
                        <ui:inputSelectOption text="EST" label="EST" value="true"/>
                        <ui:inputSelectOption text="VOD" label="VOD" />   
                    </ui:inputSelect>    
                </div>
                <div class="slds-col--padded slds-size--1-of-6">
                    National vs. iTunes:
                    <ui:inputSelect aura:id="filter" multiple="true" value="{!v.accountFilter}" change="{!c.onFilterChange}" >
                        
                        <ui:inputSelectOption text="National" label="National" value="true"/>
                        <ui:inputSelectOption text="iTunes" label="iTunes" value="true" />   
                    </ui:inputSelect>    
                </div>
                
                <div class="slds-col--padded slds-size--1-of-6">
                    Standard vs TPR:
                    <ui:inputSelect aura:id="filter" multiple="true" value="{!v.StandardorTPRFilter}" change="{!c.onFilterChange}" >
                        
                        <ui:inputSelectOption text="Standard" label="Standard" value="true" />
                        <ui:inputSelectOption text="TPR" label="TPR" value="true"/>   
                    </ui:inputSelect>
                </div>
                
                
                
                
    
                
                
            </div>
            <div id="tableParent" class="slds-card__body slds-show slds-m-left_small slds-m-right_small slds-scrollable" style="{!v.showViewAll ? v.numberOfRecords > 6 ? 'height:215px;resize:vertical;min-height:230px;' : 'height:157px;resize:vertical;min-height:180px;' : 'height:400px;resize:vertical;min-height:200px;'}">            
                <table id="{!'tblTPRPricing' + v.showViewAll}" class="fht-table slds-table slds-table_bordered slds-no-row-hover slds-table_cell-buffer " >
                    <thead id="theadScroll1">
                        <tr class="slds-text-title_caps">
                            <th class="tableBorder" scope="col" style="z-index: 5;">
                                <div class="slds-truncate" title="Product Name">Action</div>
                            </th>
                            <th class="tableBorder" scope="col" style="z-index: 5;">
                                <span onclick="{!c.sort}" id="productName">
                                    <div class="slds-truncate" title="Product Name">Product Name
                                        <span aura:id="productName" > 
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>                     
                            <!-- 1/9/2019 -->
                            <th class="tableBorder" scope="col" style="z-index: 4;">
                                <span onclick="{!c.sort}" id="startEndDiff">
                                    <div class="slds-truncate" title="Days Since Last TPR"> Last TPRd
                                        <span aura:id="startEndDiff" > 
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            
                            <th class="tableBorder" scope="col" style="z-index: 4;">  
                                <span onclick="{!c.sort}" id="startDate">
                                    <div class="slds-truncate" title="Start Date">Start Date
                                        <span aura:id="startDate" class="slds-hide">
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            <th class="tableBorder" scope="col" style="z-index: 4;"> 
                                <span onclick="{!c.sort}" id="endDate">
                                    <div class="slds-truncate" title="End Date">End Date
                                        <span aura:id="endDate" class="slds-hide">
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.sd),v.wsp),v.national),v.Standard) ?  'z-index:4;' : 'display:none;'}">
                                  <span onclick="{!c.sort}" id="usESTSDWSP">
                                    <div class="slds-truncate" title="US EST SD WSP">US EST SD WSP
                                        <span aura:id="usESTSDWSP" >
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.sd),v.wsp),v.national),v.TPR) ?   'z-index:4;' : 'display:none;'}" bgcolor="#FFFF99">
                                 <span onclick="{!c.sort}" id="usESTSDWSPTPR">
                                    <div class="slds-truncate" title="US EST SD WSP TPR" style="font-weight:bold"> US EST SD WSP TPR
                                        <span aura:id="usESTSDWSPTPR" >
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.sd),v.srp),v.national),v.Standard)  ? 'z-index:4;' : 'display:none;'}">
                                 <span onclick="{!c.sort}" id="usESTSDSRP">
                                    <div class="slds-truncate" title="US EST SD SRP">US EST SD SRP
                                        <span aura:id="usESTSDSRP" >
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.sd),v.srp),v.national),v.TPR) ? 'z-index:4;' : 'display:none;'}" bgcolor="#FFFF99">
                                 <span onclick="{!c.sort}" id="usESTSDSRPTPR">
                                    <div class="slds-truncate" title="US EST SD SRP TPR" style="font-weight:bold">US EST SD SRP TPR
                                        <span aura:id="usESTSDSRPTPR" >
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.hd),v.wsp),v.national),v.Standard)  ? 'z-index:4;' : 'display:none;'}">
                                <span onclick="{!c.sort}" id="usESTHDWSP">
                                    <div class="slds-truncate" title="US EST HD WSP">US EST HD WSP
                                        <span aura:id="usESTHDWSP" >
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.hd),v.wsp),v.national),v.TPR)  ? 'z-index:4;' : 'display:none;'}" bgcolor="#FFFF99">
                                 <span onclick="{!c.sort}" id="usESTHDWSPTPR">
                                    <div class="slds-truncate" title="US EST HD WSP TPR"  style="font-weight:bold">US EST HD WSP TPR
                                        <span aura:id="usESTHDWSPTPR" >
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.hd),v.srp),v.national),v.Standard)  ? 'z-index:4;' : 'display:none;'}">
                                 <span onclick="{!c.sort}" id="usESTHDSRP">
                                    <div class="slds-truncate" title="US EST HD SRP">US EST HD SRP
                                        <span aura:id="usESTHDSRP" >
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.hd),v.srp),v.national),v.TPR)  ? 'z-index:4;' : 'display:none;'}" bgcolor="#FFFF99">
                               <span onclick="{!c.sort}" id="usESTHDSRPTPR">
                                    <div class="slds-truncate" title="US EST HD SRP TPR"  style="font-weight:bold">US EST HD SRP TPR
                                        <span aura:id="usESTHDSRPTPR" >
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.uhd),v.wsp),v.national),v.Standard)  ? 'z-index:4;' : 'display:none;'}">
                                 <span onclick="{!c.sort}" id="usESTUHDWSP">
                                    <div class="slds-truncate" title="US EST UHD WSP">US EST UHD WSP
                                        <span aura:id="usESTUHDWSP" >
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.uhd),v.wsp),v.national),v.TPR)  ? 'z-index:4;' : 'display:none;'}" bgcolor="#FFFF99">
                               <span onclick="{!c.sort}" id="usESTUHDWSPTPR">
                                    <div class="slds-truncate" title="US EST UHD WSP TPR"  style="font-weight:bold">US EST UHD WSP TPR
                                        <span aura:id="usESTUHDWSPTPR" >
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.uhd),v.srp),v.national),v.Standard)  ? 'z-index:4;' : 'display:none;'}">
                                <span onclick="{!c.sort}" id="usESTUHDSRP">
                                    <div class="slds-truncate" title="US EST UHD SRP">US EST UHD SRP
                                        <span aura:id="usESTUHDSRP" >
                                            <aura:if isTrue="{!v.assending}">
                                                <lightning:icon iconName="utility:arrowup" size="xx-small"/>
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowdown" size="xx-small"/>
                                                </aura:set>
                                            </aura:if> 
                                        </span>
                                    </div>
                                </span>
                            </th>
                            <th class="tableBorder" scope="col"  style="{!and(and(and(and(and(v.us,v.est),v.uhd),v.srp),v.national),v.TPR)  ? 'z-index:4;' : 'display:none;'}" bgcolor="#FFFF99">
                                 <span onclick="{!c.sort}" id="usESTUHDSRPTPR">
